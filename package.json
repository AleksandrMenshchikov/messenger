{
  "name": "messenger",
  "version": "1.0.0",
  "description": "messenger",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "dev": "rm -rf dist && parcel src/index.html --no-cache --open",
    "build": "rm -rf dist && npm run lint && parcel build src/index.html --no-cache",
    "start": "node app.ts",
    "prepare": "husky install",
    "lintjs": "./node_modules/.bin/eslint .",
    "lintcss": "./node_modules/.bin/stylelint \"**/*.css\"",
    "lint": "npm run lintjs && npm run lintcss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AleksandrMenshchikov/messenger.git"
  },
  "keywords": [
    "messenger"
  ],
  "author": "Aleksandr Menshchikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AleksandrMenshchikov/messenger/issues"
  },
  "homepage": "https://github.com/AleksandrMenshchikov/messenger#readme",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@types/express": "4.17.15",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "eslint": "8.29.0",
    "eslint-config-airbnb": "19.0.4",
    "husky": "8.0.0",
    "parcel": "2.8.2",
    "postcss": "8.4.20",
    "postcss-import": "15.1.0",
    "stylelint": "14.16.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-selector-bem-pattern": "2.1.1"
  },
  "dependencies": {
    "express": "4.18.2",
    "nanoid": "4.0.0"
  }
}
