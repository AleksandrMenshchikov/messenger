{
  "name": "messenger",
  "version": "1.0.0",
  "description": "messenger",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "dev": "parcel src/pages/index.hbs --open",
    "build": "rm -rf dist && npm run test && parcel build src/pages/index.hbs --no-cache",
    "start": "node app.js",
    "lintjs": "./node_modules/.bin/eslint .",
    "lintcss": "./node_modules/.bin/stylelint \"**/*.css\"",
    "prepare": "husky install",
    "test": "npm run lintjs && npm run lintcss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AleksandrMenshchikov/messenger.git"
  },
  "keywords": [
    "messenger"
  ],
  "author": "Aleksandr Menshchikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AleksandrMenshchikov/messenger/issues"
  },
  "homepage": "https://github.com/AleksandrMenshchikov/messenger#readme",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@types/express": "4.17.14",
    "@types/node": "18.11.13",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "eslint": "8.29.0",
    "eslint-config-airbnb": "19.0.4",
    "husky": "8.0.0",
    "parcel": "2.8.0",
    "postcss-import": "15.0.0",
    "stylelint": "14.16.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-selector-bem-pattern": "2.1.1"
  },
  "dependencies": {
    "express": "4.18.2",
    "uuid": "^9.0.0"
  }
}
